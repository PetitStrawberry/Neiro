target 'Neiro (iOS)' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Neiro (iOS)
  pod 'CocoaUPnP', :git => 'https://github.com/PetitStrawberry/CocoaUPnP', :branch => 'develop'
  pod 'LicensePlist'

end

